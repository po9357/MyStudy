<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>MovieProject</display-name>
  <welcome-file-list>
    <welcome-file>MovieMain.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>movie.SignUp</servlet-name>
    <servlet-class>com.movie.SignUp</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>movie.SignUp</servlet-name>
    <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>movie.Login</servlet-name>
    <servlet-class>com.movie.LogIn</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>movie.Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>movie.booking</servlet-name>
    <servlet-class>com.movie.Booking</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>movie.booking</servlet-name>
    <url-pattern>/booking</url-pattern>
  </servlet-mapping>
</web-app>